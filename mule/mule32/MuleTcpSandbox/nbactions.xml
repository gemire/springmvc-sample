<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>CUSTOM-install</actionName>
            <displayName>install-to-mule-server</displayName>
            <goals>
                <goal>mule:mule</goal>
                <goal>mule:install</goal>
                <goal>antrun:run</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-compile no tests</actionName>
            <displayName>compile no tests</displayName>
            <goals>
                <goal>compile</goal>
            </goals>
            <properties>
                <skipTests>true</skipTests>
            </properties>
        </action>
    </actions>
